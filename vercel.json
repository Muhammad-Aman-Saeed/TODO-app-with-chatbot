{
  "version": 2,
  "framework": "nextjs",
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "cd frontend && npm install --legacy-peer-deps && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}